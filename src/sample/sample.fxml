<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" prefHeight="500.0" prefWidth="500.0" style="-fx-background-color: linear-gradient(to right top, #1703e5, #0069ff, #0099ff, #00c0f7, #57e1e7);" vgap="10" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="500.0" prefWidth="500.0">
         <children>
            <Button fx:id="btn1" alignment="CENTER" layoutX="172.0" layoutY="217.0" mnemonicParsing="false" onAction="#showsites" prefHeight="33.0" prefWidth="147.0" style="-fx-background-color: linear-gradient(to right top, #16da30, #21d82c, #2ad527, #30d323, #36d11e);" text="COVID SITES">
               <font>
                  <Font name="Georgia Bold Italic" size="13.0" />
               </font></Button>
            <Button fx:id="btn2" layoutX="172.0" layoutY="118.0" mnemonicParsing="false" onAction="#stats" prefHeight="33.0" prefWidth="147.0" style="-fx-background-color: linear-gradient(to right top, #16da30, #21d82c, #2ad527, #30d323, #36d11e);" text="STATS">
               <font>
                  <Font name="Georgia Bold" size="14.0" />
               </font></Button>
            <Button fx:id="btn3" layoutX="172.0" layoutY="167.0" mnemonicParsing="false" onAction="#symptom" prefHeight="33.0" prefWidth="147.0" style="-fx-background-color: linear-gradient(to right top, #16da30, #21d82c, #2ad527, #30d323, #36d11e);" text="SELF ANALYSIS" textOverrun="WORD_ELLIPSIS">
               <font>
                  <Font name="Georgia Bold" size="14.0" />
               </font></Button>
            <Button fx:id="advisoriesbutton" layoutX="269.0" layoutY="276.0" mnemonicParsing="false" onAction="#viewadvisories" prefHeight="33.0" prefWidth="189.0" style="-fx-background-color: linear-gradient(to right top, #16da30, #21d82c, #2ad527, #30d323, #36d11e);" text="View Covid-19 Advisories" textFill="#000000f5">
               <font>
                  <Font name="Georgia Bold" size="12.0" />
               </font></Button>
            <Button fx:id="helplinebutton" layoutX="24.0" layoutY="276.0" mnemonicParsing="false" onAction="#viewhelpline" prefHeight="33.0" prefWidth="189.0" style="-fx-background-color: linear-gradient(to right top, #16da30, #21d82c, #2ad527, #30d323, #36d11e);" text="View Covid-19 helplines">
               <font>
                  <Font name="Georgia Bold" size="12.0" />
               </font></Button>
            <Button fx:id="refbtn" layoutX="171.0" layoutY="429.0" mnemonicParsing="false" onAction="#Refresh" prefHeight="33.0" prefWidth="157.0" style="-fx-background-color: linear-gradient(to right top, #16da30, #21d82c, #2ad527, #30d323, #36d11e);" text="REFRESH">
            <font>
               <Font name="Georgia Bold" size="12.0" />
            </font></Button>
            <Button fx:id="shownews" layoutX="173.0" layoutY="326.0" mnemonicParsing="false" onAction="#showNews" prefHeight="33.0" prefWidth="147.0" style="-fx-background-color: linear-gradient(to right top, #16da30, #21d82c, #2ad527, #30d323, #36d11e);" text="NEWS">
               <font>
                  <Font name="Georgia Bold" size="12.0" />
               </font></Button>
            <Label alignment="CENTER" layoutX="108.0" layoutY="8.0" prefHeight="86.0" prefWidth="274.0" text="COVID TRACKER">
               <font>
                  <Font name="Algerian" size="36.0" />
               </font>
            </Label>
            <Button fx:id="graphbtn" layoutX="168.0" layoutY="377.0" mnemonicParsing="false" onAction="#showgraph" prefHeight="33.0" prefWidth="157.0" style="-fx-background-color: linear-gradient(to right top, #16da30, #21d82c, #2ad527, #30d323, #36d11e);" text="Graphical Analysis">
               <font>
                  <Font name="Georgia Bold" size="14.0" />
               </font></Button>
         </children>
      </Pane>
   </children>
</GridPane>
