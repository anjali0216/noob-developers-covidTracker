<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Districtwise">
    <children>
        <Button fx:id="search" layoutX="108.0" layoutY="250.0" mnemonicParsing="false" onAction="#searchdistrict" text="Search" />
        <TextField fx:id="stateds" layoutX="108.0" layoutY="178.0" prefHeight="25.0" prefWidth="213.0" promptText="Enter state" />
        <TextField fx:id="district" layoutX="108.0" layoutY="214.0" prefHeight="25.0" prefWidth="213.0" promptText="Enter district" />

        <Label fx:id="recDisplay" layoutX="108.0" layoutY="293.0" prefHeight="182.0" prefWidth="284.0" />
        <Button fx:id="prev" layoutX="334.0" layoutY="14.0" mnemonicParsing="false" onAction="#prevPg" text="&lt;- Back To Previous Page" />
      <Label layoutX="108.0" layoutY="141.0" prefHeight="25.0" prefWidth="277.0" text="To display stats of a particular district, Search below" />
      <Label layoutX="108.0" layoutY="77.0" text="To display the district-wise stats of a state, Search below" />
      <TextField fx:id="statedw" layoutX="108.0" layoutY="108.0" prefHeight="25.0" prefWidth="213.0" promptText="Enter the state" />
      <Button fx:id="display" layoutX="337.0" layoutY="108.0" mnemonicParsing="false" onAction="#displayList" text="Display" />

    </children>
</AnchorPane>
