<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Calendar">
   <children>
      <Label fx:id="date" layoutX="128.0" layoutY="212.0" prefHeight="145.0" prefWidth="257.0" text=" " />
      <DatePicker fx:id="picker" layoutX="88.0" layoutY="109.0" prefHeight="26.0" prefWidth="324.0" promptText="Pick a date" />
      <Button fx:id="showdate" layoutX="221.0" layoutY="165.0" mnemonicParsing="false" onAction="#displaydate" text="Show stats" />
      <Button fx:id="prev" layoutX="379.0" layoutY="416.0" mnemonicParsing="false" onAction="#prevPg" text="&lt;- Previous Page" />
   </children>
</AnchorPane>
