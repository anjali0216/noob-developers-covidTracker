<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: linear-gradient(to right top, #1703e5, #0069ff, #0099ff, #00c0f7, #57e1e7);" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Allgraph">
   <children>
      <Button fx:id="worldgraph" layoutX="128.0" layoutY="103.0" mnemonicParsing="false" onAction="#viewworldgraph" style="-fx-background-color: linear-gradient(to right top, #16da30, #21d82c, #2ad527, #30d323, #36d11e);" text="View Covid-19 Cases for Major Countries">
         <font>
            <Font name="Arial Rounded MT Bold" size="12.0" />
         </font></Button>
      <Button fx:id="indiagraph" layoutX="126.0" layoutY="152.0" mnemonicParsing="false" onAction="#viewindiagraph" prefHeight="25.0" prefWidth="254.0" style="-fx-background-color: linear-gradient(to right top, #16da30, #21d82c, #2ad527, #30d323, #36d11e);" text="View Overall  Covid-19 Stats for India">
         <font>
            <Font name="Arial Rounded MT Bold" size="12.0" />
         </font></Button>
      <Button fx:id="statewisegraph" layoutX="126.0" layoutY="200.0" mnemonicParsing="false" onAction="#viewstategraph" prefHeight="25.0" prefWidth="254.0" style="-fx-background-color: linear-gradient(to right top, #16da30, #21d82c, #2ad527, #30d323, #36d11e);" text="View Statewise Stats for India">
         <font>
            <Font name="Arial Rounded MT Bold" size="12.0" />
         </font></Button>
      <Button fx:id="growthchart" layoutX="126.0" layoutY="254.0" mnemonicParsing="false" onAction="#viewgrowthchart" prefHeight="25.0" prefWidth="254.0" style="-fx-background-color: linear-gradient(to right top, #16da30, #21d82c, #2ad527, #30d323, #36d11e);" text="View GrowthChart of covid in India">
         <font>
            <Font name="Arial Rounded MT Bold" size="12.0" />
         </font></Button>
      <Button fx:id="hmbtn" layoutX="261.0" layoutY="340.0" mnemonicParsing="false" onAction="#backhome1" style="-fx-background-color: linear-gradient(to right top, #16da30, #21d82c, #2ad527, #30d323, #36d11e);" text="&lt;- Back TO HOME">
         <font>
            <Font name="Arial Rounded MT Bold" size="12.0" />
         </font></Button>
      <Button fx:id="wholestats" layoutX="126.0" layoutY="56.0" mnemonicParsing="false" onAction="#showwholestats" style="-fx-background-color: linear-gradient(to right top, #16da30, #21d82c, #2ad527, #30d323, #36d11e);" text="View Overall Covid-19 Stats for the World" textAlignment="JUSTIFY">
         <font>
            <Font name="Arial Rounded MT Bold" size="12.0" />
         </font></Button>
   </children>
</AnchorPane>
