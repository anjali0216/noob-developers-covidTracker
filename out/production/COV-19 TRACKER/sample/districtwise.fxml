<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" style="-fx-background-color: linear-gradient(to right top, #1703e5, #0069ff, #0099ff, #00c0f7, #57e1e7);" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Districtwise">
    <children>
        <Button fx:id="search" layoutX="108.0" layoutY="250.0" mnemonicParsing="false" onAction="#searchdistrict" style="-fx-background-color: linear-gradient(to right top, #16da30, #21d82c, #2ad527, #30d323, #36d11e);" text="Search">
         <font>
            <Font name="Arial Rounded MT Bold" size="12.0" />
         </font></Button>
        <TextField fx:id="stateds" layoutX="108.0" layoutY="178.0" prefHeight="25.0" prefWidth="213.0" promptText="Enter state" />
        <TextField fx:id="district" layoutX="108.0" layoutY="214.0" prefHeight="25.0" prefWidth="213.0" promptText="Enter district" />

        <Label fx:id="recDisplay" layoutX="108.0" layoutY="293.0" prefHeight="182.0" prefWidth="284.0" />
        <Button fx:id="prev" layoutX="334.0" layoutY="14.0" mnemonicParsing="false" onAction="#prevPg" style="-fx-background-color: linear-gradient(to right top, #16da30, #21d82c, #2ad527, #30d323, #36d11e);" text="&lt;- Back To Previous Page">
         <font>
            <Font name="Arial Rounded MT Bold" size="12.0" />
         </font></Button>
      <Label layoutX="108.0" layoutY="142.0" prefHeight="25.0" prefWidth="310.0" text="To display stats of a particular district, Search below">
         <font>
            <Font name="Arial Rounded MT Bold" size="12.0" />
         </font></Label>
      <Label layoutX="108.0" layoutY="55.0" prefHeight="37.0" prefWidth="331.0" text="To display the district-wise stats of a state, Search below">
         <font>
            <Font name="Arial Rounded MT Bold" size="12.0" />
         </font></Label>
      <TextField fx:id="statedw" layoutX="108.0" layoutY="108.0" prefHeight="25.0" prefWidth="213.0" promptText="Enter the state" />
      <Button fx:id="display" layoutX="337.0" layoutY="108.0" mnemonicParsing="false" onAction="#displayList" style="-fx-background-color: linear-gradient(to right top, #16da30, #21d82c, #2ad527, #30d323, #36d11e);" text="Display">
         <font>
            <Font name="Arial Rounded MT Bold" size="12.0" />
         </font></Button>

    </children>
</AnchorPane>
